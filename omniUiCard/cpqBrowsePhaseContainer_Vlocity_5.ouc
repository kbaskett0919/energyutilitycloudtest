    <stylingConfiguration>{&quot;customStyles&quot;:&quot;.cpq__browse-phase-container{\n    background-color: #F3F3F3;\n    margin-top: -8px;\n    border-top: 1px solid #C9C9C9;\n}\n.cpq__preview-cart_close{\n    flex:1 1 auto;\n}\n.cpq__browse_sidebar_container{\n    padding-top: 20px;\n    min-height: 100vh;\n    background-color: #ffffff;\n    border-right: 1px solid #C9C9C9;\n}\n.cpq__preview-cart-container{\n    min-height: 100vh;\n}\n.omniscript-body {\n    .cpq__parent-hierarchy_name,\n    .cpq__flex-table-hierarchy_name {\n      text-align: left;\n    }\n  }\n  \n  /* cpqBrowsePhaseContainer */\n  .cpq__product-view_container {\n    flex: 1 1 auto;\n  }\n  @media (max-width: 728px){\n    .cpq__browse_sidebar_container{\n        min-height: unset;\n    }\n  }\n  @media (max-width: 1024px) {\n    /* cpqProductFilterChild */\n    .category-mobile {\n      ul {\n        overflow-x: auto;\n        display: flex;\n        height: 50px;\n      }\n  \n      ul::-webkit-scrollbar {\n        display: none;\n      }\n  \n      li {\n        display: inline;\n      }\n  \n      span {\n        display: inline;\n      }\n  \n      .tree-selector__item-span_active {\n        box-shadow: inset 0 -0.25rem 0 var(--lwc-brandPrimary, rgb(21, 137, 238));\n      }\n    }\n  }\n  /* cpqProductFilterChild */\n  .custom-btn {\n    width: fit-content;\n    margin-left: -1px;\n    border-radius: 0px;\n  }\n  \n  /* cpqProductFilterChild */\n  .filter-footer-btn .slds-button {\n    border-radius: 0px;\n  }\n  \n  /* CPQAttributeConfigFields_filters */\n  span.slds-radio_faux,\n  span.slds-checkbox_faux {\n    margin-left: 25px !important;\n  }\n  \n  .checkbox-title-style .slds-form-element__legend {\n    font-size: 12px;\n    color: #3e3e3c;\n    font-weight: bold;\n  }\n  \n  .text-attribute-title label {\n    font-size: 12px;\n    color: #3e3e3c;\n    font-weight: bold;\n    padding-bottom: 20px;\n    padding-top: 0px;\n  }\n  \n  .cpq-checkbox-style {\n    span {\n      padding-l